{
    "coc.preferences.extensionUpdateCheck": "weekly",
    "coc.preferences.rootPatterns": [
        ".vim",
        ".git",
        ".hg",
        ".projections.json"
    ],
    "coc.source.omni.filetypes": ["VimspectorPrompt"],
    "suggest.noselect": false,
    "suggest.maxCompleteItemCount": 50,
    "suggest.minTriggerInputLength": 2,
    "diagnostic.displayByAle": true,
    "list.source.files.args": ["--hidden", "--files"],
    "python.jediEnabled": true,
    "python.linting.enabled": false,
    "python.formatting.provider": "yapf",
    "python.analysis.logLevel": "Error",
    "highlight.document.enable": true,
    "snippets.priority": 100,
    "snippets.snipmate.enable": false,
    "snippets.ultisnips.enable": false,
    "snippets.loadFromExtensions": true,
    "git.enableGutters": true,
    "git.addedSign.text": "▎",
    "git.changedSign.text": "░",
    "git.removedSign.text": "▏",
    "git.changeRemovedSign.text": "▒",
    "git.topRemovedSign.text": "▔",
    "git.addedSign.hlGroup": "GitGutterAdd",
    "git.changedSign.hlGroup": "GitGutterChange",
    "git.removedSign.hlGroup": "GitGutterDelete",
    "git.changeRemovedSign.hlGroup": "GitGutterChangeDelete",
    "git.topRemovedSign.hlGroup": "GitGutterDelete",
    "git.signOffset": 1,
    "yank.list.maxsize": 50,
    "coc-actions.hideCursor": false,
    "http.proxy": "http://127.0.0.1:8123",
    "explorer.icon.enableNerdfont": true,
    "explorer.icon.customIcons": {
        "icons": {
            "vimwiki": {
                "code": "ﴬ",
                "color": "#F07178"
            },
            "vim": {
                "code": "",
                "color": "#86B300"
            }
        },
        "extensions": {
            "wiki": "vimwiki",
            "vim": "vim"
        },
        "filenames": {},
        "dirnames": {},
        "patternMatches": {},
        "dirPatternMatches": {}
    },
    "explorer.quitOnOpen": true,
    "explorer.git.icon.status.modified": "✹",
    "explorer.git.icon.status.added": "✚",
    "explorer.git.icon.status.unmerged": "═",
    "explorer.git.icon.status.renamed": "➜",
    "explorer.git.icon.status.deleted": "✖",
    "explorer.git.icon.status.untracked": "",
    "explorer.git.icon.status.copied": "",
    "explorer.git.icon.status.mixed": "✭",
    "explorer.git.icon.status.unmodified": " ",
    "explorer.git.icon.status.ignored": "﯏",
    "explorer.sources": [
        {
            "name": "buffer",
            "expand": true
        },
        {
            "name": "file",
            "expand": true
        }
    ],
    "explorer.keyMappings.global": {
        "<C-h>": "toggleHidden",
        "[g": "gitPrev",
        "]g": "gitNext"
    },
    "zi.patterns": {
        "javascript": [
            "^\\s*\\/\\/",
            "^\\s*\\/\\*",
            "^\\s*\\*"
        ],
        "typescript": [
            "^\\s*\\/\\/",
            "^\\s*\\/\\*",
            "^\\s*\\*"
        ],
        "markdown": [],
        "vimwiki.markdown": [],
        "vimwiki": [],
        "vim": [
            "^\\s*\\\""
        ],
        "gitcommit": [],
        "python": [
            "^\\s*\\\"\\\"\\\"",
            "^\\s*#",
            "^\\s*:",
            "^\\s*[A-Z]"
        ]
    },
    "todolist.promptForReminder": false,
    "cSpell.enabledLanguageIds": [
        "vim",
        "asciidoc",
        "c",

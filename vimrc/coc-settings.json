{
    "suggest.noselect": false,
    "suggest.maxCompleteItemCount": 150,
    "suggest.minTriggerInputLength": 2,
    "diagnostic.displayByAle": true,
    "python.jediEnabled": false,
    "python.linting.enabled": false,
    "python.formatting.provider": "yapf",
    "highlight.document.enable": true,
    "snippets.priority": 100,
    "snippets.snipmate.enable": false,
    "snippets.loadFromExtensions": false,
    "git.addedSign.hlGroup": "GitGutterAdd",
    "git.changedSign.hlGroup": "GitGutterChange",
    "git.removedSign.hlGroup": "GitGutterDelete",
    "git.changeRemovedSign.hlGroup": "GitGutterChangeDelete",
    "git.signOffset": 1,
    "yank.list.maxsize": 50,
    "http.proxy": "127.0.0.1:8123",
    "coc-actions.hideCursor": false,
    "explorer.icon.enableNerdfont": true,
    "explorer.quitOnOpen": false,
    "languageserver": {
        "ccls": {
            "command": "ccls",
            "filetypes": ["c", "cpp", "cuda", "objc", "objcpp"],
            "rootPatterns": [".ccls-root", "compile_commands.json"],
            "initializationOptions": {
                "cache": {
                    "directory": ".ccls-cache"
                },
                "client": {
                    "snippetSupport": true
                }
            }
        }
    }
}

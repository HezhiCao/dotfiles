    "cSpell.dictionaries": [
        "pythonTerms",
        "commonTerms"
    ],
    "cSpell.allowCompoundWords": true,
    "cSpell.showStatus": false,
    "leetcode.language": "cpp",
    "languageserver": {
        "ccls": {
            "command": "ccls",
            "filetypes": ["c", "cpp", "cuda"],
            "rootPatterns": [".ccls-root", ".vim", "compile_commands.json"],
            "initializationOptions": {
                "cache": {
                    "directory": ".vim/.ccls-cache"
                },
                "client": {
                    "snippetSupport": false
                },
                "highlight": {
                    "lsRanges" : true
                }
            }
        }
    }
}

{
    "coc.preferences.extensionUpdateCheck": "weekly",
    "coc.preferences.rootPatterns": [
        ".vim",
        ".git",
        ".hg",
        ".projections.json"
    ],
    "coc.preferences.formatOnSaveFiletypes": [
        "json"
    ],
    "coc.source.omni.filetypes": [
        "VimspectorPrompt"
    ],
    "suggest.noselect": false,
    "suggest.maxCompleteItemCount": 50,
    "suggest.minTriggerInputLength": 2,
    "suggest.completionItemKindLabels": {
        "class": "囹",
        "color": "",
        "constant": "",
        "constructor": "",
        "default": "",
        "enum": "",
        "enumMember": "",
        "event": "鬒",
        "field": "綠",
        "file": "",
        "folder": "",
        "function": "ﳣ",
        "interface": "",
        "keyword": "",
        "method": "",
        "module": "",
        "operator": "洛",
        "property": "",
        "reference": "渚",
        "snippet": "",
        "struct": "פּ",
        "text": "",
        "typeParameter": "璉",
        "unit": "",
        "value": "",
        "variable": ""
    },
    "diagnostic.errorSign": "->",
    "diagnostic.warningSign": "--",
    "diagnostic.infoSign": "--",
    "diagnostic.hintSign": "··",
    "diagnostic.enableHighlightLineNumber": false,
    "diagnostic.format": "%message\n[%source]",
    "codeLens.separator": "",
    "rust-analyzer.inlayHints.typeHintsSeparator": " ",
    "signature.maxWindowHeight": 10,
    "signature.preferShownAbove": false,
    "list.source.files.args": [
        "--hidden",
        "--files"
    ],
    "highlight.document.enable": true,
    "highlight.disableLanguages": [
        "vimwiki"
    ],
    "snippets.priority": 100,
    "snippets.snipmate.enable": false,
    "snippets.ultisnips.enable": false,
    "snippets.loadFromExtensions": true,
    "git.enableGutters": true,
    "git.addedSign.text": "▎",
    "git.changedSign.text": "░",
    "git.removedSign.text": "▏",
    "git.changeRemovedSign.text": "▒",
    "git.topRemovedSign.text": "▔",
    "git.addedSign.hlGroup": "GitGutterAdd",
    "git.changedSign.hlGroup": "GitGutterChange",
    "git.removedSign.hlGroup": "GitGutterDelete",
    "git.changeRemovedSign.hlGroup": "GitGutterChangeDelete",
    "git.topRemovedSign.hlGroup": "GitGutterDelete",
    "git.signPriority": 1,
    "git.branchCharacter": "",
    "git.changedDecorator": "✹ ",
    "git.stagedDecorator": "● ",
    "git.untrackedDecorator": " ",
    "git.showCommitInFloating": true,
    "yank.list.maxsize": 50,
    "coc-actions.hideCursor": false,
    "explorer.icon.enableNerdfont": true,
    "explorer.icon.customIcons": {
        "icons": {
            "vimwiki": {
                "code": "ﴬ",
                "color": "#F07178"
            },
            "vim": {
                "code": "",
                "color": "#86B300"
            }
        },
        "extensions": {
            "wiki": "vimwiki",
            "vim": "vim"
        },
        "filenames": {},
        "dirnames": {},
        "patternMatches": {},
        "dirPatternMatches": {}
    },
    "explorer.quitOnOpen": true,
    "explorer.git.icon.status.modified": "✹",
    "explorer.git.icon.status.added": "✚",
    "explorer.git.icon.status.unmerged": "═",
    "explorer.git.icon.status.renamed": "➜",
    "explorer.git.icon.status.deleted": "✖",
    "explorer.git.icon.status.untracked": "",
    "explorer.git.icon.status.copied": "",
    "explorer.git.icon.status.mixed": "✭",
    "explorer.git.icon.status.unmodified": " ",
    "explorer.git.icon.status.ignored": "﯏",
    "explorer.sources": [
        {
            "name": "buffer",
            "expand": true
        },
        {
            "name": "file",
            "expand": true
        }
    ],
    "explorer.keyMappings.global": {
        "<C-h>": "toggleHidden",
        "[g": "gitPrev",
        "]g": "gitNext",
        "cw": "rename",
        "df": "deleteForever",
        "dt": "delete"
    },
    "zi.hover": false,
    "zi.patterns": {
        "javascript": [
            "^\\s*\\/\\/",
            "^\\s*\\/\\*",
            "^\\s*\\*"
        ],
        "typescript": [
            "^\\s*\\/\\/",
            "^\\s*\\/\\*",
            "^\\s*\\*"
        ],
        "markdown": [],
        "vimwiki.markdown": [],
        "vimwiki": [],
        "text": [],
        "vim": [
            "^\\s*\\\""
        ],
        "gitcommit": [],
        "python": [
            "^\\s*\\\"\\\"\\\"",
            "^\\s*#",
            "^\\s*:",
            "^\\s*[A-Z]"
        ]
    },
    "cSpell.enabledLanguageIds": [
        "vim",
        "asciidoc",
        "c",
        "cpp",
        "csharp",
        "css",
        "git-commit",
        "go",
        "handlebars",
        "haskell",
        "html",
        "jade",
        "java",
        "javascript",
        "javascriptreact",
        "json",
        "jsonc",
        "latex",
        "less",
        "markdown",
        "vimwiki.markdown",
        "vimwiki",
        "php",
        "plaintext",
        "python",
        "pug",
        "restructuredtext",
        "rust",
        "scala",
        "scss",
        "text",
        "typescript",
        "typescriptreact",
        "yaml",
        "yml"
    ],
    "cSpell.dictionaries": [
        "commonTerms",
        "pythonTerms",
        "cFamilyTerms"
    ],
    "cSpell.allowCompoundWords": true,
    "cSpell.showStatus": false,
    "cSpell.diagnosticLevel": "Hint",
    "translator.engines": [
        "bing",
        "youdao",
        "haici"
    ],
    "leetcode.language": "cpp",
    "clangd.semanticHighlighting": false,
    "clangd.arguments": [
        "--completion-style=detailed",
        "--suggest-missing-includes",
        "--clang-tidy",
        "--header-insertion=never"
    ],
    "jedi.completion.disableSnippets": true,
    "diagnostic-languageserver.mergeConfig": true,
    "diagnostic-languageserver.linters": {
        "cppcheck": {
            "command": "cppcheck",
            "args": [
                "--language=c++",
                "--template={line}:{column} {severity}: {message}",
                "--enable=style",
                "--force",
                "--inconclusive",
                "--quiet",
                "%file"
            ],
            "debounce": 100,
            "sourceName": "cppcheck",
            "isStderr": true,
            "isStdout": false,
            "offsetLine": 0,
            "offsetColumn": 0,
            "formatPattern": [
                "^(\\d+):(\\d+) ([a-z]+): (.*)$",
                {
                    "line": 1,
                    "column": 2,
                    "security": 3,
                    "message": 4
                }
            ],
            "securities": {
                "error": "error",
                "warning": "warning",
                "style": "info",
                "performance": "warning",
                "portability": "warning",
                "information": "hint"
            }
        },
        "pylint": {
            "args": [
                "--output-format",
                "text",
                "--score",
                "no",
                "--msg-template",
                "'{line}:{column}:{category}:{msg} ({msg_id}:{symbol})'",
                "--rcfile=~/.pylintrc",
                "%file"
            ]
        },
        "vint": {
            "args": [
                "--format",
                "{severity}:{line_number}:{column_number}: {description} (see {reference})",
                "--enable-neovim",
                "--style-problem",
                "--no-color",
                "-"
            ],
            "formatPattern": [
                "(\\w+):(\\d+):(\\d+):\\s*(.*)$",
                {
                    "security": 1,
                    "line": 2,
                    "column": 3,
                    "message": 4
                }
            ],
            "securities": {
                "style_problem": "info",
                "warning": "warning",
                "error": "error"
            }
        },
        "write-good": {
            "args": [
                "--no-passive",
                "--text=%text"
            ],
            "securities": {
                "undefined": "hint"
            }
        },
        "cpplint": {
            "args": [
                "--filter=-build/include_subdir,-build/header_guard,-build/c++11",
                "%file"
            ],
            "securities": {
                "1": "hint",
                "2": "info",
                "3": "info",
                "4": "warning",
                "5": "warning"
            }
        },
        "languagetool": {
            "command": "cpulimit",
            "args": [
                "-i",
                "-l",
                "20",
                "languagetool",
                "-l",
                "en",
                "-d",
                "WORD_CONTAINS_UNDERSCORE,DASH_RULE,WHITESPACE_RULE,ARROWS",
                "%file"
            ],
            "formatPattern": [
                "^\\d+?\\.\\)\\s+Line\\s+(\\d+),\\s+column\\s+(\\d+),\\s+Rule\\s+ID:\\s+([^\\n]+)\nMessage:\\s+(.*)(\\r|\\n)*$",
                {
                    "line": 1,
                    "column": 2,
                    "message": [
                        4,
                        " [",
                        3,
                        "]"
                    ]
                }
            ],
            "securities": {
                "undefined": "hint"
            }
        }
    },
    "diagnostic-languageserver.trace.server": "verbose",
    "diagnostic-languageserver.debug": true,
    "diagnostic-languageserver.filetypes": {
        "sh": "shellcheck",
        "cpp": [
            "cpplint",
            "cppcheck"
        ],
        "python": [
            "pylint"
        ],
        "vim": "vint",
        "vimwiki": [
            "write-good",
            "languagetool"
        ],
        "markdown": [
            "write-good",
            "languagetool"
        ],
        "text": [
            "write-good",
            "languagetool"
        ]
    },
    "json.schemas": [
        {
            "fileMatch": [
                "*.vimspector.json"
            ],
            "url": "https://puremourning.github.io/vimspector/schema/vimspector.schema.json"
        },
        {
            "fileMatch": [
                ".gadgets.json",
                ".gadgets.d/*.json"
            ],
            "url": "https://puremourning.github.io/vimspector/schema/gadgets.schema.json"
        }
    ],
    "languageserver": {
        "asm-lsp": {
            "command": "asm-lsp",
            "filetypes": [
                "gas",
                "asm"
            ]
        }
    }
}
